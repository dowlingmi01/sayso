            "lookuplabel": "quota",
            "default": "--Please Select percentile--",
            "displaywhen": [
                "add",
                "grid",
                "detail",
                "edit"
            ]
        },
        {
            "colname": "gender_id",
            "type": "fkey",
            "lookuptable": "lookup_gender",
            "lookupfield": "id",
            "lookuplabel": "short_name",
            "default": "--Please Select a gender--",
            "displaywhen": [
                "add",
