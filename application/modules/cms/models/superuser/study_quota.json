{
    "table": [
        {
            "tablename": "Study_Quota",
            "model":"Study_Quota",
            "permissions":[
            	"allowgrid",
            	"allowadd",
            	"allowdelete",
            	"allowedit",
            	"disallowdetails"
            ]
        }
    ],
    "columns": [
        {
            "colname": "id",
            "type": "hidden",
            "width": 5,
            "displaywhen": [
                "grid",
                "detail",
                "edit"
            ]
        },
        {
            "colname": "study_id",
            "type": "fkey",
            "lookuptable": "study",
            "lookupfield": "id",
            "lookuplabel": "name",
            "default": "--Please Select a study--",
            "displaywhen": [
                "add",
                "grid",
                "detail",
                "edit"
            ]
        },
        {
            "colname": "percentile_id",
            "type": "fkey",
            "lookuptable": "lookup_quota_percentile",
